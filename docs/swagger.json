        },
        "version": "1.0"
    },
    "host": "api.inlive.app",
    "basePath": "/v1",
    "paths": {
        "/stream/init_session": {
            "post": {
                "description": "Init session send offer session to WebRTC",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "stream"
                ],
                "summary": "Init session stream",
                "parameters": [
                    {
                        "description": "Body Request",
                        "name": "body",
                        "in": "body",
                        "schema": {
                            "$ref": "#/definitions/models.InitSessionRequest"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "$ref": "#/definitions/models.ResponseSwag"
                        }
                    }
                }
            }
        }
    },
    "definitions": {
        "models.InitSessionRequest": {
            "type": "object",
            "required": [
                "offer_session",
                "room_id",
                "user_id"
            ],
            "properties": {
                "offer_session": {
                    "type": "string",
                    "example": "nmhbf93y21592r3fhjgoewhgveb28356932kffkl"
                },
                "room_id": {
                    "type": "string",
                    "example": "9918"
                },
                "user_id": {
                    "type": "string",
                    "example": "adhon.rizky@gmail.com"
                }
            }
        },
        "models.ResponseSwag": {
            "type": "object",
            "properties": {
                "code": {
                    "type": "string",
                    "example": "00"
                },
                "data": {
                    "$ref": "#/definitions/models.ResponseSwagInitSess"
                },
                "message": {
                    "type": "string",
                    "example": "Successfully init session and get answer session"
                },
                "response_id": {
                    "type": "string",
                    "example": "00"
                }
            }
        },
        "models.ResponseSwagInitSess": {
            "type": "object",
            "properties": {
                "answer_session": {
                    "type": "string",
                    "example": ""
                },
                "pid": {
                    "type": "string",
                    "example": "19681"
                },
                "port_audio": {
                    "type": "string",
                    "example": "51361"
                },
                "port_video": {
                    "type": "string",
                    "example": "56827"
                }
            }
        }
    }
}